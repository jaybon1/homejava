package simple;

public class ForEx01 {
	public static void main(String[] args) {
		
		for (int i = 0; i < 10; i++) {
			
			if(i == 2) {
				System.out.println("continue 실행");
				continue;
				
			} else if(i == 7) {
				System.out.println("break 실행");
				break;
				
			}
			System.out.println(i);
		}
	}
}
