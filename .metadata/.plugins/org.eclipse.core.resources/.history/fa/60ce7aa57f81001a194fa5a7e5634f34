package simple;

import java.util.ArrayList;
import java.util.HashSet;

public class Self1 {
	public static void main(String[] args) {
		
		ArrayList<Integer> arr1 = new ArrayList<>();
		HashSet<Integer> arr2 = new HashSet<>();
		
		for (int i = 1; i < 5000; i++) {
			arr1.add(i);
		}
		
		for (int j = 1; j < 5000; j++) {
			int temp = j;
			int temp1 = 0;
			Integer sum = j;
			for (int i = 0; i < Integer.toString(j).length() ; i++) {
				temp1 = (int)(temp/(Math.pow(10, Integer.toString(temp).length() - 1)));
				sum = sum + temp1;
				temp =(int)(temp%(Math.pow(10, Integer.toString(temp).length() - 1)));
			}
			arr2.add(sum);
		}
		
		for (Integer integer : arr2) {
			for (Integer integer2 : arr1) {
				
			}
		}
	}
}
