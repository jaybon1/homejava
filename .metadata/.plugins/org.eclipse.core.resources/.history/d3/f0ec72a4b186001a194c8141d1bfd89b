package example;

// 모듈화 하기
class Data {
	
	private String info;
	
	public String getInfo() {
		String info;
		
		
		return "정보";
	}

}

// main에서는 모듈에서 가공한 정보를 가져와서 쓰기만 하면된다
public class Data1 {

	public static void main(String[] args) {
		
		Data a = new Data();

		System.out.println(a.getInfo());

	}
}


