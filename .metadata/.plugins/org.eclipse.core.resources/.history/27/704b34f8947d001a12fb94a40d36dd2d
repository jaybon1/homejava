package ch02;

class 우리집 {
	String 집이름;
	int 평수;
	int 층수;
	String 주소;
	
	public void sound() {
		
		System.out.println("멍멍");
		
	}
	

	public 우리집(int 평수, String 집이름, int 층수, String 주소) {
		this.집이름 = 집이름;
		this.평수 = 평수;
		this.층수 = 층수;
		this.주소 = 주소;
	}
	
	public 우리집(int 평수, String 집이름) {
		this(평수, 집이름, 10, "모름");
	}
	
	public 우리집() {
		this(24, "없음");
		this.sound();
	}
	
	
	
	
}

public class Class1 {
	public static void main(String[] args) {
		우리집 a = new 우리집();
		
//		System.out.println(a.집이름);
//		System.out.println(a.평수);
//		System.out.println(a.층수);
//		System.out.println(a.주소);

		
	}
}

