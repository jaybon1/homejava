package test;

public class Test {
	public static void main(String[] args) {
		
		for (int i = 2; i < 10; i++) {
			System.out.println(i + " * " + 1 + " = " + i*1 + "\n" +
			i + " * " + 2 + " = " + i*2 + "\n" +
			i + " * " + 3 + " = " + i*3 + "\n" +
			i + " * " + 4 + " = " + i*4 + "\n" +
			i + " * " + 5 + " = " + i*5 + "\n" +
			i + " * " + 6 + " = " + i*6 + "\n" +
			i + " * " + 7 + " = " + i*7 + "\n" +
			i + " * " + 8 + " = " + i*8 + "\n" +
			i + " * " + 9 + " = " + i*9);
		}
		
	}
}
