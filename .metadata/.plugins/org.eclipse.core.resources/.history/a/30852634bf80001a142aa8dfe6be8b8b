package simple;

public class ForEx02 {
	public static void main(String[] args) {
		int temp;
		
		
		int arr[] = new int[10];
		
		temp = 0;
		for (int i = 0; i < arr.length; i++) {

			System.out.println(arr[i] = i);
			
		}
		
		
		
		
		int arr1[] = new int[10];
		
		temp = 0;
		for (int i : arr1) {
			
			temp++;
			
			System.out.println(i = temp);
			
		}

	}
}
